---
pagination:
  data: contributors
  size: 250
  alias: contributor
eleventyExcludeFromCollections: true
permalink: /admin/pronomi/index.html
---
{% extends 'layouts/admin.njk' %}

{% block content %}
	<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
	<main>
		<header>
			<h1>pronomi</h1>
		</header>
		<section>
			<div id="piechart"></div>
			<div id="piechart2"></div>
			<script type="text/javascript">
			// Load google charts
			google.charts.load('current', {'packages':['corechart']});
			google.charts.setOnLoadCallback(drawChart);

			// Draw the chart and set the chart values
			function drawChart() {
				var data = google.visualization.arrayToDataTable({{ contributors[0].pronounsArrayString | safe }});

				// Optional; add a title and set the width and height of the chart
				var options = {
					'title':'kittieba', 'width': 850, 'height': 650,
					slices: {
            0: { color: '#B7A33D' },
            1: { color: '#3DB766' },
            2: { color: '#3D51B7' },
            3: { color: '#B73D8E' },
          }
				};

				// Display the chart inside the <div> element with id="piechart"
				var chart = new google.visualization.PieChart(document.getElementById('piechart'));
				chart.draw(data, options);
			}

			google.charts.setOnLoadCallback(drawChart2);

			// Draw the chart and set the chart values
			function drawChart2() {
				var data = google.visualization.arrayToDataTable({{ stories[0].pronounWordsArrayString | safe }});

				// Optional; add a title and set the width and height of the chart
				var options = { 'title':'kliem', 'width': 850, 'height': 650,
					slices: {
            0: { color: '#B7A33D' },
            1: { color: '#3DB766' },
            2: { color: '#3D51B7' },
            3: { color: '#B73D8E' },
          }
				};

				// Display the chart inside the <div> element with id="piechart"
				var chart2 = new google.visualization.PieChart(document.getElementById('piechart2'));
				chart2.draw(data, options);
			}
			</script>
			<div id="piechart"></div>
		</section>	</main>
{% endblock %}
