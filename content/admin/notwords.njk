---
permalink: /admin/kliem-mhux-suppost/index.html
---
{% extends 'layouts/admin.njk' %}

{% block content %}
	<main>
		<section>
			<link href="/pagefind/pagefind-ui.css" rel="stylesheet">
			<style>
					body {
					--pagefind-ui-scale: 1;
					--pagefind-ui-primary: #034ad8;
					--pagefind-ui-text: #393939;
					--pagefind-ui-background: #ffffff;
					--pagefind-ui-border: #eeeeee;
					--pagefind-ui-tag: #eeeeee;
					--pagefind-ui-border-width: 2px;
					--pagefind-ui-border-radius: 2px;
					--pagefind-ui-image-border-radius: 2px;
					--pagefind-ui-image-box-ratio: 3 / 2;
					--pagefind-ui-font: "Alegreya Sans";
				}
				#search {
					background-color: #ffffff;
					position: fixed;
					width: 33%;
					top: 4rem;
					right: 1rem;
				}
				.suggested-only {
					color: #ccc;
				}
			</style>
			<div id="search" class="search"></div>
			<script src="/pagefind/pagefind-ui.js" onload="new PagefindUI({ element: '#search', showImages: false, autofocus: true });"></script>
			<table>
			{% for entry in styleguide.entries | selectattr("notWords") %}
				{% for notWord in entry.notWords %}
					{% for story in stories %}
						{% if notWord and story.vocabulary.indexOf(notWord) > -1 %}
							<tr{% if entry.attributes.definition.indexOf("Nippreferu ") > -1 %} class="suggested-only"{% endif %}>
								<td><strong>{{ notWord }}</strong></td>
								<td><a href="https://aphroconfuso.mt/{{ story.slug }}#:~:text={{ notWord }}" target="_new">{{ story.reportingTitle }}</a></td>
								<td>{{ entry.attributes.term }}</td>
							</tr>
						{% endif %}
					{% endfor %}
				{% endfor %}
			{% endfor %}
			</table>
			<hr>
			<table>
			{% for story in stories | sort(false, false, 'sortTitle') %}
				{% for entry in styleguide.entries | selectattr("notWords") %}
					{% for notWord in entry.notWords %}
						{% if notWord and story.vocabulary.indexOf(notWord) > -1 %}
							<tr{% if entry.attributes.definition.indexOf("Nippreferu ") > -1 %} class="suggested-only"{% endif %}>
								<td><strong>{{ notWord }}</strong></td>
								<td><a href="https://aphroconfuso.mt/{{ story.slug }}#:~:text={{ notWord }}" target="_new">{{ story.reportingTitle }}</a></td>
								<td>{{ entry.attributes.term }}</td>
							</tr>
						{% endif %}
					{% endfor %}
				{% endfor %}
			{% endfor %}
			</table>
		</section>
	</main>
{% endblock %}
