{% extends 'layouts/base.njk' %}

{% block head %}
	<title>{{ process.title }} · Aphroconfuso</title>
	<meta content="{{ metadata.description }}" name="description">
	<meta property="og:image" content="{{ metadata.settings.imagesUrl }}{{ metadata.settings.socialImage }}">
{% endblock %}

{% block content %}
	<article class="generic">
		<header>
			<ul><li class="header-label">&nbsp;</li></ul>
			<h1>{{ process.title }}</h1>
			<h2>&nbsp;</h2>
		</header>
		<div class="container">
		<section id="grid-body">
			<div class="generic-body-text">{{ process.body | prettifyMaltese | safe }}</div>
		</section>
		<section id="grid-max-panel-left-top">
			{% include "_menu_generic.njk" %}
		</section>
	</div>
	</article>
{% endblock %}
